{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    {
      "identifier": "opener:default",
      "allow": [
        { "url": "https://console.serendb.com/**" },
        { "url": "https://docs.serendb.com/**" },
        { "url": "https://*.serendb.com/**" }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://api.serendb.com/**" }
      ]
    }
  ]
}
